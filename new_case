SUM(
  CASE 
    WHEN sf_fund_type = 'ASC' AND sf_business_segment_sfdc = 'Select'           THEN sf_total_members * 46.77
    WHEN sf_fund_type = 'ASC' AND sf_business_segment_sfdc = 'Small Group'      THEN sf_total_members * 31.36
    WHEN sf_fund_type = 'ASC' AND sf_business_segment_sfdc = 'Public & Labor'   THEN sf_total_members * 23.87
    WHEN sf_fund_type = 'ASC' AND sf_business_segment_sfdc = 'Key'              THEN sf_total_members * 31.32
    WHEN sf_fund_type = 'ASC' AND sf_business_segment_sfdc = 'National Accounts' THEN sf_total_members * 27.88
    WHEN sf_fund_type = 'Risk' AND sf_business_segment_sfdc = 'Select'           THEN sf_total_members * 51.06
    WHEN sf_fund_type = 'Risk' AND sf_business_segment_sfdc = 'Small Group'      THEN sf_total_members * 48.60
    WHEN sf_fund_type = 'Risk' AND sf_business_segment_sfdc = 'Public & Labor'   THEN sf_total_members * 26.99
    WHEN sf_fund_type = 'Risk' AND sf_business_segment_sfdc = 'Key'              THEN sf_total_members * 42.75
    WHEN sf_fund_type = 'Risk' AND sf_business_segment_sfdc = 'National Accounts' THEN sf_total_members * 77.80
    ELSE 0
  END
)